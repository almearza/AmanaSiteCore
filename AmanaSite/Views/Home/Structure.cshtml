<!--// Page Wrapper Start //-->
@using Microsoft.AspNetCore.Mvc.Localization;
@inject IViewLocalizer Localizer
@{
    var secBg = Url.Content("~/img/bg/structure-sec-img.png");
}
<div class="page-wrapper" id="wrapper">
    <!--// Main Area Start //-->
    <main class="main-area">
        <!--// Breadcrumb Section Start //-->
        <section class="breadcrumb-section section" data-scroll-index="1" data-bg-image-path="@secBg">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8">

                    </div>
                </div>
            </div>
        </section>
        <!--// Breadcrumb Section end //-->
        <div id="DetialsPage">
            <div class="div-details">
                <div class="div-header">
                    <a href="~/"><i class="fa fa-home"></i></a>
                    <i class="fa fa-chevron-circle-left"></i>
                    <p>@Localizer["Structure"]</p>
                </div>
                <div class="div-top-lable">
                    <i class="fa fa-sitemap"></i>
                    <span>@Localizer["AmanaStructure"]</span>
                    <div class="top-left-icon">
                        <a href="#" title='@Localizer["FacebookSharing"]' target="_blank" id="facebook-sharing">
                            <i class="fab fa-facebook-f"></i>
                        </a>
                        <a href="#" title='@Localizer["TwitterSharing"]' target="_blank" id="twitter-sharing">
                            <i class="fab fa-twitter"></i>
                        </a>
                    </div>
                </div>
                <div class="page-details">
                    <!--// Resume Section Start //-->
                    <section class="section pb-minus-76">
                        <div class="container">
                            <div class="row">
                               <div class="col-12">
                                    <iframe src="https://lms.amana-md.gov.sa/AmanaOrgChart/Default.aspx"
                                    style="width:100%;height:70vh; font-family: 'DroidArabicKufiBold' !important"></iframe>
                               </div>
                            </div>
                        </div>
                    </section>
                    <!--// Resume Section End //-->
                </div>
            </div>
        </div>



    </main>
    <!--// Main Area End //-->
</div>
<!--// Page Wrapper End //-->